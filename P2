##bmi calculation and if-elif-else funtion

height = float(input('plz enter your height:'))
weight = float(input('plz enter your weight:'))

bmi = weight / (height ** 2)

if 0 < bmi < 100 :
    if bmi < 18.5 :
        print('underweight')
    elif 18.5 <= bmi < 25 :
        print('normal')
    elif 25 <= bmi < 28 :
        print('overweight')
    elif 28 <= bmi < 32 :
        print('obesity')
    else :
        print('serious obesity')

else :
    print('number incorrect')
    pass
